{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@soundx/db": [
        "../../packages/db/dist"
      ],
      "@soundx/services": [
        "../../packages/services/src"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "scripts/withNetworkSecurityConfig.js"
  ]
}